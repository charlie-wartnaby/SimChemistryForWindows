begin_state;
temperature 300 ;
gravity 0 ;
autorun 1 ;
grid_on 1 ;
gridsize 0.05 ;
simchemistry_ver 2 ;
deltatime 1e-005 ;
use_auto_deltatime 1 ;
avg_time_const 0.003 ;
sim_size left 0 bottom 0.25 width 3.765 height 2.25;

end_state;

begin_history;
temperature 500;
objtype textbox name "textbox1" font "Times New Roman" size 16;
textbox "textbox1" xleft 0.1 ytop 2.4 text "red + blue <--> green + green      Ea = 5 kJ/mol" id 1;
objtype wall name "thermal wall" linethick 2 system_thermal thermal linered 128 linegreen 0 lineblue 64;
