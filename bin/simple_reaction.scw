objtype mol name "green" mass 20 radius 0.05 form_energy -10 linethick 0.2 fillred 0 fillgreen 255 fillblue 0 linered 0 linegreen 0 lineblue 0;
fillmol "red" left 0.273526 top 2.1272 right 0.94125 bottom 1.65255  speed_sys_thermal direction_random first_id 1 last_id 30;
fillmol "blue" left 0.265481 top 1.35087 right 0.973429 bottom 0.884263  speed_sys_thermal direction_random first_id 1 last_id 30;
objtype monitor name "monitor1"  hatch 0 label 1 linethick 2 red 255 green 0 blue 0;
monitor "monitor1" moltype "red" nofixt 100 nofixdens 10 moltype "blue" nofixt 100 nofixdens 10 moltype "green" nofixt 100 nofixdens 10  xleft 0.2 xright 1.7 ytop 2.2 ybottom 0.6  id 1 mon_uid 1;
objtype graph name "graph1";
graph "graph1" xdata "sim:time" ydata "monitor1:1:green:number" xleft 2.300000 ytop 2.400000 xright 3.700000 ybottom 1.700000 title "Number of green versus time (ns)" filled 1 size 10 style circle red 255 green 0 blue 0  id 1;
select "thermal wall" 5;
